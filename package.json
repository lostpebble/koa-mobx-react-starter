{
  "name": "koa-mobx-react-starter",
  "version": "1.0.0",
  "license": "Unlicense",
  "repository": "https://github.com/lostpebble/koa-mobx-react-bootstrap",
  "description": "A straightforward starter for Node javascript web projects. Using Koa, MobX, Pug and ReactJS (with universal / isomorphic server rendering)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --max-executable-size=256 --max-old-space-size=256 --max-semi-space-size=3 ./built/server/server-starter.js",
    "monitor": "nodemon --max-executable-size=256 --max-old-space-size=256 --max-semi-space-size=3 ./built/server/server-starter.js",
    "build-server": "rimraf ./built && cross-env BABEL_ENV=server babel ./src -d ./built",
    "build-client": "cross-env NODE_ENV=production BABEL_ENV=client-build webpack --config webpack.config.client.js",
    "build": "npm run build-server && npm run build-client",
    "build-start": "npm run build && npm run start",
    "build-monitor": "npm run build && npm run monitor",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server/babel-server.js"
  },
  "author": "Paul Myburgh",
  "dependencies": {
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.6",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0-alpha.4",
    "koa-compose": "^4.0.0",
    "koa-compress": "^2.0.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "marko": "^4.2.8",
    "mobx": "^3.1.2",
    "mobx-react": "^4.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.5",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "es6-map": "^0.1.4",
    "es6-symbol": "^3.1.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "image-webpack-loader": "^3.3.0",
    "koa-webpack-middleware": "^1.0.1",
    "mobx-react-devtools": "^4.2.6",
    "node-sass": "^4.5.2",
    "nodemon": "^1.10.2",
    "postcss-loader": "^1.3.3",
    "regenerator-runtime": "^0.10.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
