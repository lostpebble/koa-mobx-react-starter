{
  "name": "koa-mobx-react-starter",
  "version": "1.0.0",
  "repository": "https://github.com/lostpebble/koa-mobx-react-bootstrap",
  "description": "A straightforward starter for Node javascript web projects. Using Koa, MobX, Pug and ReactJS (with universal / isomorphic server rendering)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./src/server/babel-server.js",
    "monitor": "nodemon ./src/server/babel-server.js",
    "build": "cross-env NODE_ENV=production BABEL_ENV=client-build webpack --config webpack.config.client.js",
    "buildstart": "npm run build && npm run start",
    "buildmonitor": "npm run build && npm run monitor",
    "dev": "cross-env NODE_ENV=development nodemon ./src/server/babel-server.js"
  },
  "author": "Paul Myburgh",
  "license": "ISC",
  "dependencies": {
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.6",
    "isomorphic-fetch": "^2.2.1",
    "koa": "^2.0.0-alpha.4",
    "koa-compose": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-mount": "^2.0.0",
    "koa-pug": "^3.0.0-0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "mobx": "^2.5.2",
    "mobx-react": "^3.5.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.14.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^11.1.0",
